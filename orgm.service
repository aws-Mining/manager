[Unit]
Description=My Application Service
After=network.target
Wants=network.target

[Service]
Type=simple
User=root
Group=root

WorkingDirectory=/etc/aws_org/
ExecStart=/etc/aws_org/orgm
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal
SyslogIdentifier=orgm

[Install]
WantedBy=multi-user.target
